{
    "name": "tasko-products/symfony-prometheus-exporter",
    "description": "Generic metrics exporter for Symfony including messenger event metrics via middleware, request metrics, logging metrics and custom metrics via the Collector composition",
    "type": "symfony-bundle",
    "require": {
        "php": "~8.3",
        "symfony/framework-bundle": "^6.4|^7.0",
        "promphp/prometheus_client_php": "^2.14",
        "symfony/messenger": "^6.4|^7.0",
        "tasko-products/php-codestyle": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "TaskoProducts\\SymfonyPrometheusExporterBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TaskoProducts\\SymfonyPrometheusExporterBundle\\Tests\\": "tests/"
        }
    },
    "homepage": "https://www.tasko.de/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Rotermund",
            "email": "lukas.rotermund@tasko.de",
            "role": "Developer"
        },
        {
            "name": "Tobias Lorenz",
            "email": "tobias.lorenz@tasko.de",
            "role": "Developer"
        }
    ]
}
